{
  "GeneralSettings": {
    "IssuerUrl": "https://localhost:7296"
  },
  "ConnectionStrings": {
    "IdentityConnection": "Database=IdentityDatabase;Username=postgres;Password=123;Server=localhost;Port=5432"
  },
  "IntegrationSettings": {
    "LogisticsApiBaseUrl": "https://localhost:7085"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.EntityFrameworkCore.Database.Command": "Information",
        "OpenIddict": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 7
        }
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "OpenIddict": "Debug"
    }
  }
}